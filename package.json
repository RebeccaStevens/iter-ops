{
  "name": "iter-ops",
  "version": "1.6.4",
  "description": "Basic operations on iterables",
  "sideEffects": false,
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "check-format": "prettier --list-different \"./**/*.ts\"",
    "check-spelling": "cspell --config=.cspell.json \"**/*.{md,ts}\"",
    "doc": "typedoc --readme none",
    "format": "prettier --write \"./**/*.ts\"",
    "lint": "eslint .",
    "test": "cross-env TS_NODE_PROJECT=tsconfig.test.json nyc mocha -r ts-node/register 'test/**/*.spec.ts'"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/vitaly-t/iter-ops",
  "repository": {
    "type": "git",
    "url": "https://github.com/vitaly-t/iter-ops.git"
  },
  "bugs": {
    "url": "https://github.com/vitaly-t/iter-ops/issues",
    "email": "vitaly.tomilov@gmail.com"
  },
  "keywords": [
    "typescript",
    "synchronous",
    "asynchronous",
    "sequence",
    "iterators",
    "iterables",
    "collections",
    "map",
    "filter",
    "reduce"
  ],
  "author": {
    "name": "Vitaly Tomilov",
    "email": "vitaly.tomilov@gmail.com"
  },
  "contributors": [
    {
      "name": "Vitaly Tomilov",
      "email": "vitaly.tomilov@gmail.com"
    },
    {
      "name": "Rebecca Stevens",
      "email": "rebecca.stevens@outlook.co.nz"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@rollup/plugin-node-resolve": "14.1.0",
    "@rollup/plugin-typescript": "8.5.0",
    "@types/chai": "4.3.3",
    "@types/mocha": "10.0.0",
    "@types/node": "18.7.23",
    "@types/rollup-plugin-auto-external": "2.0.2",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "chai": "4.3.6",
    "cross-env": "7.0.3",
    "cspell": "6.10.1",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "mocha": "10.0.0",
    "nyc": "15.1.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "rollup": "2.79.1",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-dts": "4.2.2",
    "rollup-plugin-gzip": "3.0.1",
    "rollup-plugin-terser": "7.0.2",
    "ts-node": "10.9.1",
    "tslib": "2.4.0",
    "typedoc": "0.23.15",
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@3.2.3"
}
